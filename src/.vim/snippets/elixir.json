{
  ".source.elixir": {
    "module declaration": {
      "prefix": "dm",
      "body": "defmodule $1 do\n\t$2\nend",
      "description": "Snippet for module declaration"
    },
    "function declaration": {
      "prefix": "df",
      "body": "def $1 do\n\t$2\nend",
      "description": "Snippet for function declaration"
    },
    "private function declaration": {
      "prefix": "dfp",
      "body": "defp $1 do\n\t$2\nend",
      "description": "Snippet for private function declaration"
    }
  }
}
